read_html('https://www.worldometers.info/geography/7-continents/') %>%
html_nodes('table') %>%
html_table() %>%
.[[1]] %>%
clean_names() %>%
mutate(across(
.cols = -continent & where(is.character),
.fns = ~parse_number(.x)
)) %>%
kableExtra::kbl(caption = "World Population by Continent",
booktabs = TRUE) %>%
kable_classic_2(font_size = 8,
full_width = FALSE)
